
.number_type N

.decl n( x : N )
.decl m( x : N )

n(1).
n(2).
n(3).

m(4).
m(5).
m(6).

.type pair = [ first : N, second : N ]

.decl p ( p1 : pair, x: N, p2:pair, y:N )
p(*pair[x,y], as(x*y,N), *pair[x,y], as(y/x,N)) :- n(x), m(y).

.decl r ( x1 : N, y1 : N, z1:N, x2 : N, y2 : N, z2:N )
.output r ()
r(x1,y1,z1, x2, y2, z2) :- p(*pair[x1,y1], z1, *pair[x2,y2], z2).
